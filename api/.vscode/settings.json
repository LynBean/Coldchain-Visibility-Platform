{
  "files.exclude": {
    ".pytest_cache": true,
    ".ruff_cache": true,
    ".venv": true,
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/Thumbs.db": true
  },
  "python.analysis.extraPaths": ["${workspaceFolder}/.venv/lib/python3.13/site-packages"],
  "python.analysis.ignore": ["${workspaceFolder}/.venv"],
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python"
}
